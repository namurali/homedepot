pipeline {
  agent any
	stages {
    			stage ('One') {      
        			steps {
					         echo "Hi, this is murali"
				      }	
			}
    			stage ('Two') {      
        			steps {
					         input "Do you want to proceed"
			        }	
			 }
			stage ('Three') {`
					 steps {
						echo "Done... last stage"
					 }
			}
			+
		}
}
